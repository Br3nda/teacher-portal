<h2>Students page </h2>

<thead>
  <tr>
    <th> Name </th>
    <th> Class room </th>
    <th> current level </th>
  </tr>
</thead>

<% @students.each do |student| %>

  <tr>
    <td><%= student.name %></td>
    <td><%= student.class_room %></td>
    <td><%= student.current_level %></td>
  </tr>
<% end %>